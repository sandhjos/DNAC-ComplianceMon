<CustomPolicy xmlns="http://www.cisco.com/nccm/api/schemas/1.1" id="_ISB_IOS_XE_2_1_1_21_9_2021_12_14_35_265">
    <Title><![CDATA[ISB_IOS_XE_2-1-1-2]]></Title>
    <Description><![CDATA[Set version 2 for 'ip ssh version']]></Description>
    
    <CreatedOn><![CDATA[1630516475358]]></CreatedOn>
    <ModifiedOn><![CDATA[1630532745965]]></ModifiedOn>
    <CreatedUser><![CDATA[admin]]></CreatedUser>
    <PolicyType><![CDATA[WIZARD]]></PolicyType>
    <Categories>
       <Category id="_CCM_USER_DEFINED" type="category">CCM_USER_DEFINED</Category>
    </Categories>
    <VersionSelectorRefs>
      <VersionSelectorRef><![CDATA[iosxe]]></VersionSelectorRef>
    </VersionSelectorRefs>
    <References>
      <Reference identifier="Custom">
        <Source>Custom</Source>
        <Sections>Custom Sections</Sections>
        <Versions>Custom Versions</Versions>
      </Reference>
    </References>
    <Rules>
      <Rule identifier="_ISB_IOS_2_1_1_2">
        <Title><![CDATA[ISB_IOS_XE_2-1-1-2]]></Title>
        <Description><![CDATA[version 2 for 'ip ssh version' is set]]></Description>
        <VersionSelectorRefs>
          <VersionSelectorRef>iosxe</VersionSelectorRef>
        </VersionSelectorRefs>
         <RuleType>RAW_CONFIG</RuleType>
        <Conditions>
          <Condition>
            <Type>RAW_CONFIG</Type>
            <DisplayString><![CDATA[Configuration must contain the string ip ssh version 2]]></DisplayString>
            <MatchAction>CONTINUE</MatchAction>
            <NonMatchAction>RAISE_AND_CONTINUE</NonMatchAction>
            <BlockParams>
              <PassCriterion>ALL</PassCriterion>
              <RaiseViolationForAllInstances>true</RaiseViolationForAllInstances>
            </BlockParams>
            <RegexpParams>
              <PassCriterion>ALL</PassCriterion>
              <RaiseViolationForAllInstances>true</RaiseViolationForAllInstances>
            </RegexpParams>
            <Severity>1</Severity>
            <ViolationMessage><![CDATA[MISSING ip ssh version 2]]></ViolationMessage>
          <Violation>
            <Message><![CDATA[MISSING ip ssh version 2]]></Message>
            </Violation>
            <Scope>ALL_CONFIG</Scope>
            <Operator>CONTAINS</Operator>
            <Value><![CDATA[ip ssh version 2]]></Value>
          </Condition>
        </Conditions>
      </Rule>
    </Rules>
</CustomPolicy>
